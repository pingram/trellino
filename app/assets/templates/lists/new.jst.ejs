<form id='new-list'>
  <input type="hidden" name="list[board_id]"
    value="<%= board.id %>">
  <label>Title:
    <input class='user-input' name="list[title]">
  </label>
  <br>
  <label>Rank:
    <input class='user-input' name="list[rank]">
  </label>
  <br>
  <input type="submit" class="btn btn-sm btn-primary" value="Add List">
  <button type="button" class="close-form btn-xs btn-danger">X</button>
</form>